{
  "name": "@lhci/server",
  "main": "./src/server.js",
  "version": "0.1.0",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/GoogleChrome/lighthouse-ci.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rm -rf ./dist ./storybook-static",
    "build": "npm run build:esbuild && npm run build:storybook",
    "build:esbuild": "node ../../scripts/build-app.js build ./src/ui/index.html ./dist /app/",
    "build:watch": "node ../../scripts/build-app.js watch ./src/ui/index.html ./dist /app/",
    "build:source-map-explorer": "npm run clean && npm run build && ../../scripts/source-map-explorer.sh",
    "build:storybook": "build-storybook",
    "start:storybook": "start-storybook -p 6006"
  },
  "dependencies": {
    "@lhci/utils": "0.1.0",
    "bluebird": "^3.7.2",
    "body-parser": "^1.18.3",
    "compression": "^1.7.4",
    "cron": "^1.8.2",
    "dayjs": "^1.8.28",
    "debug": "^4.3.1",
    "express": "^4.16.4",
    "express-basic-auth": "^1.2.0",
    "morgan": "^1.9.1",
    "sequelize": "^4.44.3",
    "umzug": "^2.2.0",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "clsx": "^1.0.4",
    "d3": "^5.15.0",
    "plotly.js": "^1.48.3",
    "preact": "^10.0.1",
    "preact-async-route": "^2.2.1",
    "preact-router": "^3.1.0",
    "@fontsource/material-icons": "^4.4.5",
    "@fontsource/roboto": "^4.4.5",
    "@fontsource/roboto-mono": "^4.4.5"
  },
  "alias": {
    "isomorphic-fetch": "clsx"
  }
}
